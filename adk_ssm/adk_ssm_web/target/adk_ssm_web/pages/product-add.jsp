    <div class="content-wrapper">

        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                数据管理
                <small>数据列表</small>
            </h1>
        </section>
        <!-- 内容头部 -->
        <!-- 正文区域 -->
        <div class="content" >
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    产品信息录入
                    <small>这里可以进行录入产品的信息</small>
                </h1>
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <!-- left column -->
                    <div class="col-md-12">
                        <!-- general form elements -->
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <h3 class="box-title">请输入产品信息</h3>
                            </div>
                            <!-- /.box-header -->
                            <!-- form start -->
                            <form role="form" method="post" action="/product/addProduct.do">
                                <div class="box-body">
                                    <div class="form-group">
                                        <div class="form-group">
                                            <label  class="col-sm-1 control-label">产品名</label>
                                            <div class="col-sm-3">
                                                <input type="text" class="form-control" name="productName" placeholder="产品名">
                                            </div>
                                            <label  class="col-sm-1 control-label">产品编号</label>
                                            <div class="col-sm-4">
                                                <input type="text" class="form-control" name="productNum" placeholder="产品编号">
                                            </div>
                                            <label  class="col-sm-1 control-label">城市名</label>
                                            <div class="col-sm-2">
                                                <input type="text" class="form-control" name="cityName" placeholder="城市名">
                                            </div>
                                        </div>
                                        <div class="form-group" style="padding-top: 8ch;" >
                                            <label class="col-sm-1 control-label">出发时间</label>
                                            <div  class="col-sm-6">
                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    <input type="datetime-local" class="form-control pull-right" name="departureTime">
                                                </div>
                                            </div>
                                            <label class="col-sm-1 control-label">价格</label>
                                            <div class="col-sm-4">
                                                <input type="number" class="form-control" name="productPrice" placeholder="价格">
                                            </div>
                                        </div>
                                        <div class="form-group" style="padding-top: 5ch; padding-left: 1ch;" >
                                            <label >产品介绍</label>
                                            <textarea class="form-control" rows="3" placeholder="输入产品简介....." name="productDesc"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.box-body -->

                                <div class="box-footer">
                                    <button type="submit" class="btn btn-primary pull-right" >提交</button>
                                </div>
                            </form>
                        </div>

                        <!-- /.box -->

                    </div>
                    <!--/.col (left) -->
                    <!-- right column -->

                </div>
                <!-- /.row -->
